<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - StudInfoWeb</title>
    <link rel="stylesheet" href="~/css/bootswatchTheme.css" />
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/StudInfoWeb.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">
    <link rel="stylesheet" href="//cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" />

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
            <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarColor01">
                    <ul class="navbar-nav me-auto">
                        <li class="nav-item">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Info Setup</a>
                            <div class="dropdown-menu">

                                <a class="dropdown-item" asp-area="" asp-controller="Faculty" asp-action="Index">Faculty</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Dept" asp-action="Index">Dept</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Course" asp-action="Index">Course</a>
                                
                                <div class="dropdown-divider"></div>
                                
                                <a class="dropdown-item" asp-area="" asp-controller="Student" asp-action="Index">Student</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Grade" asp-action="Index">Grade</a>
                            
                            </div>
                        </li>


                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Enrollments and Scores</a>
                            <div class="dropdown-menu">

                                <a class="dropdown-item" asp-area="" asp-controller="Enrollment" asp-action="Index">View Enrollments</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Enrollment" asp-action="AddEnrollmentClient">Course Enrollment</a>
                                
                                <div class="dropdown-divider"></div>

                                <a class="dropdown-item" asp-area="" asp-controller="Score" asp-action="Index">View Scores</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Score" asp-action="AddScoreClient">Add Scores</a>
                                
                                <div class="dropdown-divider"></div>
                                
                                <a class="dropdown-item" asp-area="" asp-controller="Score" asp-action="IndexServer">View Scores (Server)</a>
                                <a class="dropdown-item" asp-area="" asp-controller="Score" asp-action="AddScoreServer">Add Scores (Server)</a>

                                <div class="dropdown-divider"></div>

                                <a class="dropdown-item" asp-area="" asp-controller="Semester" asp-action="End">End of Semester</a>
                            </div>
                        </li>


                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Access</a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" asp-area="" asp-controller="Role" asp-action="Index">Role</a>
                                <a class="dropdown-item" asp-area="" asp-controller="UserManagement" asp-action="Index">User & Role Assignment</a>
                                <div class="dropdown-divider"></div>
                            </div>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">Reports</a>
                            <div class="dropdown-menu">
                                <a class="dropdown-item" asp-area="" asp-controller="SemesterReport" asp-action="Index">Student Semester Report</a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" asp-area="" asp-controller="SemesterReport" asp-action="IndexAllStud">All Student Semester Report</a>
                            </div>
                        </li>


                    </ul>

                    <partial name="_LoginPartial" />

                    @*<form class="d-flex">
                    <input class="form-control me-sm-2" type="text" placeholder="Search">
                    <button class="btn btn-secondary my-2 my-sm-0" type="submit">Search</button>
                    </form>*@
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            <partial name="_ValidationScriptsPartial" />
            <partial name="_Notification" />
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - StudInfoWeb - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="//cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
