@model IEnumerable<StudInfoModel.AppUser>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container p-3">
    <div class="row pt-4">
        <div class="col">
            <h2 class="text-primary">Users List</h2>
        </div>
    </div>
    <br /><br />

    <table id="tblUser" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>User Name</th>
                <th></th>
            </tr>
        </thead>


    </table>
</div>



@section Scripts{
    <script src="~/js/user.js"></script>
}